-- –ö–æ–º–ø–∞–∫—Ç–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —Å–æ–∑–¥–∞–Ω–∏—è —á–∏—Ç-—ç–ª–µ–º–µ–Ω—Ç–∞
local function CreateCompactCheatItem(name, description, yPos, hasInput, inputPlaceholder)
    local ItemFrame = Instance.new("Frame")
    local ItemBG = Instance.new("Frame")
    local NameLabel = Instance.new("TextLabel")
    local DescLabel = Instance.new("TextLabel")
    local ToggleButton = Instance.new("TextButton")
    local InputBox
    
    ItemFrame.Name = name .. "Frame"
    ItemFrame.Parent = ScrollFrame
    ItemFrame.BackgroundTransparency = 1
    ItemFrame.Position = UDim2.new(0, 8, 0, yPos)
    ItemFrame.Size = UDim2.new(1, -16, 0, hasInput and 65 or 45)
    
    ItemBG.Name = "Background"
    ItemBG.Parent = ItemFrame
    ItemBG.BackgroundColor3 = Color3.fromRGB(30, 30, 45)
    ItemBG.BorderSizePixel = 0
    ItemBG.Size = UDim2.new(1, 0, 1, 0)
    
    local BGCorner = Instance.new("UICorner")
    BGCorner.CornerRadius = UDim.new(0, 6)
    BGCorner.Parent = ItemBG
    
    NameLabel.Name = "NameLabel"
    NameLabel.Parent = ItemFrame
    NameLabel.BackgroundTransparency = 1
    NameLabel.Position = UDim2.new(0, 10, 0, 4)
    NameLabel.Size = UDim2.new(0, 180, 0, 16)
    NameLabel.Font = Enum.Font.GothamSemibold
    NameLabel.Text = name
    NameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    NameLabel.TextSize = 12
    NameLabel.TextXAlignment = Enum.TextXAlignment.Left
    
    DescLabel.Name = "DescLabel"
    DescLabel.Parent = ItemFrame
    DescLabel.BackgroundTransparency = 1
    DescLabel.Position = UDim2.new(0, 10, 0, 20)
    DescLabel.Size = UDim2.new(0, 180, 0, 14)
    DescLabel.Font = Enum.Font.Gotham
    DescLabel.Text = description
    DescLabel.TextColor3 = Color3.fromRGB(160, 160, 160)
    DescLabel.TextSize = 9
    DescLabel.TextXAlignment = Enum.TextXAlignment.Left
    
    ToggleButton.Name = "ToggleButton"
    ToggleButton.Parent = ItemFrame
    ToggleButton.BackgroundColor3 = Color3.fromRGB(220, 53, 69)
    ToggleButton.BorderSizePixel = 0
    ToggleButton.Position = UDim2.new(1, -50, 0, 8)
    ToggleButton.Size = UDim2.new(0, 45, 0, 20)
    ToggleButton.Font = Enum.Font.GothamBold
    ToggleButton.Text = "OFF"
    ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    ToggleButton.TextSize = 10
    
    local ToggleCorner = Instance.new("UICorner")
    ToggleCorner.CornerRadius = UDim.new(0, 4)
    ToggleCorner.Parent = ToggleButton
    
    if hasInput then
        InputBox = Instance.new("TextBox")
        InputBox.Name = "InputBox"
        InputBox.Parent = ItemFrame
        InputBox.BackgroundColor3 = Color3.fromRGB(45, 45, 65)
        InputBox.BorderSizePixel = 0
        InputBox.Position = UDim2.new(0, 10, 0, 40)
        InputBox.Size = UDim2.new(1, -70, 0, 20)
        InputBox.Font = Enum.Font.Gotham
        InputBox.PlaceholderText = inputPlaceholder or "–ó–Ω–∞—á–µ–Ω–∏–µ..."
        InputBox.PlaceholderColor3 = Color3.fromRGB(120, 120, 120)
        InputBox.Text = ""
        InputBox.TextColor3 = Color3.fromRGB(255, 255, 255)
        InputBox.TextSize = 10
        
        local InputCorner = Instance.new("UICorner")
        InputCorner.CornerRadius = UDim.new(0, 4)
        InputCorner.Parent = InputBox
    end
    
    return ItemFrame, ToggleButton, InputBox
end

-- –°–æ–∑–¥–∞–Ω–∏–µ –≤—Å–µ—Ö —á–∏—Ç–æ–≤ (–∫–æ–º–ø–∞–∫—Ç–Ω–æ)
local yPosition = 5
local SpeedItem, SpeedToggle, SpeedInput = CreateCompactCheatItem("‚ö° –°–∫–æ—Ä–æ—Å—Ç—å", "–ò–∑–º–µ–Ω–µ–Ω–∏–µ —Å–∫–æ—Ä–æ—Å—Ç–∏ —Ö–æ–¥—å–±—ã", yPosition, true, "16")
yPosition = yPosition + 75

local JumpItem, JumpToggle, JumpInput = CreateCompactCheatItem("ü¶ò –ü—Ä—ã–∂–æ–∫", "–£–≤–µ–ª–∏—á–µ–Ω–∏–µ —Å–∏–ª—ã –ø—Ä—ã–∂–∫–∞", yPosition, true, "50")
yPosition = yPosition + 75

local FlyItem, FlyToggle, FlySpeedInput = CreateCompactCheatItem("üöÅ –ü–æ–ª–µ—Ç", "–ü–æ–ª–µ—Ç —Å –Ω–∞—Å—Ç—Ä–æ–π–∫–æ–π —Å–∫–æ—Ä–æ—Å—Ç–∏", yPosition, true, "50")
yPosition = yPosition + 75

local NoclipItem, NoclipToggle = CreateCompactCheatItem("üëª –ù–æ–∫–ª–∏–ø", "–ü—Ä–æ—Ö–æ–¥ —Å–∫–≤–æ–∑—å —Å—Ç–µ–Ω—ã", yPosition, false)
yPosition = yPosition + 55

local ESPItem, ESPToggle = CreateCompactCheatItem("üëÅÔ∏è ESP", "–ü–æ–¥—Å–≤–µ—Ç–∫–∞ –∏–≥—Ä–æ–∫–æ–≤", yPosition, false)
yPosition = yPosition + 55

local FullbrightItem, FullbrightToggle = CreateCompactCheatItem("üí° –Ø—Ä–∫–æ—Å—Ç—å", "–ü–æ–ª–Ω–æ–µ –æ—Å–≤–µ—â–µ–Ω–∏–µ", yPosition, false)
yPosition = yPosition + 55

local InfJumpItem, InfJumpToggle = CreateCompactCheatItem("üîÑ –ë–µ—Å–∫. –ø—Ä—ã–∂–∫–∏", "–ü—Ä—ã–∂–∫–∏ –≤ –≤–æ–∑–¥—É—Ö–µ", yPosition, false)
yPosition = yPosition + 55

local ClickTPItem, ClickTPToggle = CreateCompactCheatItem("üìç –ö–ª–∏–∫ –¢–ü", "–¢–µ–ª–µ–ø–æ—Ä—Ç –ø–æ –∫–ª–∏–∫—É –º—ã—à–∏", yPosition, false)
yPosition = yPosition + 55

local SpinbotItem, SpinbotToggle, SpinSpeedInput = CreateCompactCheatItem("üåÄ –°–ø–∏–Ω–±–æ—Ç", "–í—Ä–∞—â–µ–Ω–∏–µ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞", yPosition, true, "10")
yPosition = yPosition + 75

local GodmodeItem, GodmodeToggle = CreateCompactCheatItem("üõ°Ô∏è –ë–µ—Å—Å–º–µ—Ä—Ç–∏–µ", "–ù–µ–≤–æ—Å–ø—Ä–∏–∏–º—á–∏–≤–æ—Å—Ç—å –∫ —É—Ä–æ–Ω—É", yPosition, false)
yPosition = yPosition + 55

local AutoRespawnItem, AutoRespawnToggle = CreateCompactCheatItem("‚ö∞Ô∏è –ê–≤—Ç–æ-–≤–æ–∑—Ä–æ–∂–¥–µ–Ω–∏–µ", "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–µ –≤–æ–∑—Ä–æ–∂–¥–µ–Ω–∏–µ", yPosition, false)
yPosition = yPosition + 55

local AntiAFKItem, AntiAFKToggle = CreateCompactCheatItem("üí§ –ê–Ω—Ç–∏-–ê–§–ö", "–ü—Ä–µ–¥–æ—Ç–≤—Ä–∞—â–µ–Ω–∏–µ –∫–∏–∫–∞", yPosition, false)
yPosition = yPosition + 55

local FOVItem, FOVToggle, FOVInput = CreateCompactCheatItem("üì∑ –û–±–∑–æ—Ä", "–ò–∑–º–µ–Ω–µ–Ω–∏–µ –ø–æ–ª—è –∑—Ä–µ–Ω–∏—è", yPosition, true, "70")
yPosition = yPosition + 75

local WaterWalkItem, WaterWalkToggle = CreateCompactCheatItem("üåä –•–æ–¥—å–±–∞ –ø–æ –≤–æ–¥–µ", "–•–æ–∂–¥–µ–Ω–∏–µ –ø–æ –ø–æ–≤–µ—Ä—Ö–Ω–æ—Å—Ç–∏ –≤–æ–¥—ã", yPosition, false)
yPosition = yPosition + 55

local InfYieldItem, InfYieldToggle = CreateCompactCheatItem("üîß Infinite Yield", "–ê–¥–º–∏–Ω –∫–æ–º–∞–Ω–¥—ã", yPosition, false)
yPosition = yPosition + 55

-- –û–±–Ω–æ–≤–ª—è–µ–º —Ä–∞–∑–º–µ—Ä ScrollFrame
ScrollFrame.CanvasSize = UDim2.new(0, 0, 0, yPosition + 10)

-- –†–∞—Å—à–∏—Ä–µ–Ω–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏ —á–∏—Ç–æ–≤
local function ToggleSpeed()
    CheatStates.Speed = not CheatStates.Speed
    local character = LocalPlayer.Character
    if character and character:FindFirstChild("Humanoid") then
        if CheatStates.Speed then
            local speed = tonumber(SpeedInput.Text) or 16
            character.Humanoid.WalkSpeed = speed
            SpeedToggle.Text = "ON"
            SpeedToggle.BackgroundColor3 = Color3.fromRGB(40, 167, 69)
        else
            character.Humanoid.WalkSpeed = 16
            SpeedToggle.Text = "OFF"
            SpeedToggle.BackgroundColor3 = Color3.fromRGB(220, 53, 69)
        end
    end
end

local function ToggleJumpPower()
    CheatStates.JumpPower = not CheatStates.JumpPower
    local character = LocalPlayer.Character
    if character and character:FindFirstChild("Humanoid") then
        if CheatStates.JumpPower then
            local jumpPower = tonumber(JumpInput.Text) or 50
            character.Humanoid.JumpPower = jumpPower
            JumpToggle.Text = "ON"
            JumpToggle.BackgroundColor3 = Color3.fromRGB(40, 167, 69)
        else
            character.Humanoid.JumpPower = 50
            JumpToggle.Text = "OFF"
            JumpToggle.BackgroundColor3 = Color3.fromRGB(220, 53, 69)
        end
    end
end

local function ToggleFly()
    CheatStates.Fly = not CheatStates.Fly
    local character = LocalPlayer.Character
    if character and character:FindFirstChild("HumanoidRootPart") then
        if CheatStates.Fly then
            FlySpeed = tonumber(FlySpeedInput.Text) or 50
            local BodyVelocity = Instance.new("BodyVelocity")
            BodyVelocity.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
            BodyVelocity.Velocity = Vector3.new(0, 0, 0)
            BodyVelocity.Parent = character.HumanoidRootPart
            
            FlyToggle.Text = "ON"
            FlyToggle.BackgroundColor3 = Color3.fromRGB(40, 167, 69)
            
            Connections.Fly = RunService.Heartbeat:Connect(function()
                if not CheatStates.Fly then return end
                local camera = workspace.CurrentCamera
                local moveVector = Vector3.new(0, 0, 0)
                
                if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                    moveVector = moveVector + camera.CFrame.LookVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                    moveVector = moveVector - camera.CFrame.LookVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                    moveVector = moveVector - camera.CFrame.RightVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                    moveVector = moveVector + camera.CFrame.RightVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                    moveVector = moveVector + Vector3.new(0, 1, 0)
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                    moveVector = moveVector - Vector3.new(0, 1, 0)
                end
                
                BodyVelocity.Velocity = moveVector * FlySpeed
            end)
        else
            if character.HumanoidRootPart:FindFirstChild("BodyVelocity") then
                character.HumanoidRootPart.BodyVelocity:Destroy()
            end
            if Connections.Fly then
                Connections.Fly:Disconnect()
            end
            FlyToggle.Text = "OFF"
            FlyToggle.BackgroundColor3 = Color3.fromRGB(220, 53, 69)
        end
    end
end

local function ToggleNoclip()
    CheatStates.Noclip = not CheatStates.Noclip
    if CheatStates.Noclip then
        Connections.Noclip = RunService.Stepped:Connect(function()
            local character = LocalPlayer.Character
            if character then
                for _, part in pairs(character:GetChildren()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
        end)
        NoclipToggle.Text = "ON"
        NoclipToggle.BackgroundColor3 = Color3.fromRGB(40, 167, 69)
    else
        if Connections.Noclip then
            Connections.Noclip:Disconnect()
        end
        local character = LocalPlayer.Character
        if character then
            for _, part in pairs(character:GetChildren()) do
                if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
                    part.CanCollide = true
                end
            end
        end
        NoclipToggle.Text = "OFF"
        NoclipToggle.BackgroundColor3 = Color3.fromRGB(220, 53, 69)
    end
end

local function ToggleESP()
    CheatStates.ESP = not CheatStates.ESP
    if CheatStates.ESP then
        for _, player in pairs(Players:GetPlayers()) do
            if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                local BillboardGui = Instance.new("BillboardGui")
                local NameLabel = Instance.new("TextLabel")
                local HealthBar = Instance.new("Frame")
                local HealthFill = Instance.new("Frame")
                
                BillboardGui.Name = "ESP"
                BillboardGui.Parent = player.Character.HumanoidRootPart
                BillboardGui.Size = UDim2.new(0, 120, 0, 60)
                BillboardGui.Adornee = player.Character.HumanoidRootPart
                BillboardGui.AlwaysOnTop = true
                
                NameLabel.Parent = BillboardGui
                NameLabel.Size = UDim2.new(1, 0, 0.6, 0)
                NameLabel.BackgroundTransparency = 1
                NameLabel.Text = player.Name
                NameLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
                NameLabel.TextStrokeTransparency = 0
                NameLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
                NameLabel.Font = Enum.Font.GothamBold
                NameLabel.TextSize = 12
                
                HealthBar.Name = "HealthBar"
                HealthBar.Parent = BillboardGui
                HealthBar.Position = UDim2.new(0, 0, 0.6, 0)
                HealthBar.Size = UDim2.new(1, 0, 0.4, 0)
                HealthBar.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                HealthBar.BorderSizePixel = 0
                
                HealthFill.Name = "HealthFill"
                HealthFill.Parent = HealthBar
                HealthFill.Size = UDim2.new(1, 0, 1, 0)
                HealthFill.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
                HealthFill.BorderSizePixel = 0
            end
        end
        ESPToggle.Text = "ON"
        ESPToggle.BackgroundColor3 = Color3.fromRGB(40, 167, 69)
    else
        for _, player in pairs(Players:GetPlayers()) do
            if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                local esp = player.Character.HumanoidRootPart:FindFirstChild("ESP")
                if esp then esp:Destroy() end
            end
        end
        ESPToggle.Text = "OFF"
        ESPToggle.BackgroundColor3 = Color3.fromRGB(220, 53, 69)
    end
end

local function ToggleFullbright()
    CheatStates.Fullbright = not CheatStates.Fullbright
    if CheatStates.Fullbright then
        Lighting.Brightness = 2
        Lighting.ClockTime = 12
        Lighting.FogEnd = 100000
        Lighting.GlobalShadows = false
        Lighting.Ambient = Color3.fromRGB(255, 255, 255)
        FullbrightToggle.Text = "ON"
        FullbrightToggle.BackgroundColor3 = Color3.fromRGB(40, 167, 69)
    else
        Lighting.Brightness = 1
        Lighting.ClockTime = 14.5
        Lighting.FogEnd = 100000
        Lighting.GlobalShadows = true
        Lighting.Ambient = Color3.fromRGB(70, 70, 70)
        FullbrightToggle.Text = "OFF"
        FullbrightToggle.BackgroundColor3 = Color3.fromRGB(220, 53, 69)
    end
end

local function ToggleInfiniteJump()
    CheatStates.InfiniteJump = not CheatStates.InfiniteJump
    if CheatStates.InfiniteJump then
        Connections.InfiniteJump = UserInputService.JumpRequest:Connect(function()
            local character = LocalPlayer.Character
            if character and character:FindFirstChild("Humanoid") then
                character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end)
        InfJumpToggle.Text = "ON"
        InfJumpToggle.BackgroundColor3 = Color3.fromRGB(40, 167, 69)
    else
        if Connections.InfiniteJump then
            Connections.InfiniteJump:Disconnect()
        end
        InfJumpToggle.Text = "OFF"
        InfJumpToggle.BackgroundColor3 = Color3.fromRGB(220, 53, 69)
    end
end

local function ToggleClickTP()
    CheatStates.ClickTP = not CheatStates.ClickTP
    if CheatStates.ClickTP then
        Connections.ClickTP = Mouse.Button1Down:Connect(function()
            if UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then
                local character = LocalPlayer.Character
                if character and character:FindFirstChild("HumanoidRootPart") then
                    character.HumanoidRootPart.CFrame = CFrame.new(Mouse.Hit.Position + Vector3.new(0, 5, 0))
                end
            end
        end)
        ClickTPToggle.Text = "ON"
        ClickTPToggle.BackgroundColor3 = Color3.fromRGB(40, 167, 69)
    else
        if Connections.ClickTP then
            Connections.ClickTP:Disconnect()
        end
        ClickTPToggle.Text = "OFF"
        ClickTPToggle.BackgroundColor3 = Color3.fromRGB(220, 53, 69)
    end
end

local function ToggleSpinbot()
    CheatStates.Spinbot = not CheatStates.Spinbot
    if CheatStates.Spinbot then
        SpinSpeed = tonumber(SpinSpeedInput.Text) or 10
        Connections.Spinbot = RunService.Heartbeat:Connect(function()
            local character = LocalPlayer.Character
            if character and character:FindFirstChild("HumanoidRootPart") then
                character.HumanoidRootPart.CFrame = character.HumanoidRootPart.CFrame * CFrame.Angles(0, math.rad(SpinSpeed), 0)
            end
        end)
        SpinbotToggle.Text = "ON"
        SpinbotToggle.BackgroundColor3 = Color3.fromRGB(40, 167, 69)
    else
        if Connections.Spinbot then
            Connections.Spinbot:Disconnect()
        end
        SpinbotToggle.Text = "OFF"
        SpinbotToggle.BackgroundColor3 = Color3.fromRGB(220, 53, 69)
    end
end

local function ToggleGodmode()
    CheatStates.Godmode = not CheatStates.Godmode
    local character = LocalPlayer.Character
    if character and character:FindFirstChild("Humanoid") then
        if CheatStates.Godmode then
            character.Humanoid.MaxHealth = math.huge
            character.Humanoid.Health = math.huge
            GodmodeToggle.Text = "ON"
            GodmodeToggle.BackgroundColor3 = Color3.fromRGB(40, 167, 69)
        else
            character.Humanoid.MaxHealth = 100
            character.Humanoid.Health = 100
            GodmodeToggle.Text = "OFF"
            GodmodeToggle.BackgroundColor3 = Color3.fromRGB(220, 53, 69)
        end
    end
end

local function ToggleAutoRespawn()
    CheatStates.AutoRespawn = not CheatStates.AutoRespawn
    if CheatStates.AutoRespawn then
        Connections.AutoRespawn = LocalPlayer.CharacterRemoving:Connect(function()
            wait(1)
            LocalPlayer:LoadCharacter()
        end)
        AutoRespawnToggle.Text = "ON"
        AutoRespawnToggle.BackgroundColor3 = Color3.fromRGB(40, 167, 69)
    else
        if Connections.AutoRespawn then
            Connections.AutoRespawn:Disconnect()
        end
        AutoRespawnToggle.Text = "OFF"
        AutoRespawnToggle.BackgroundColor3 = Color3.fromRGB(220, 53, 69)
    end
end

local function ToggleAntiAFK()
    CheatStates.AntiAFK = not CheatStates.AntiAFK
    if CheatStates.AntiAFK then
        Connections.AntiAFK = RunService.Heartbeat:Connect(function()
            local VirtualUser = game:service'VirtualUser'
            VirtualUser:CaptureController()
            VirtualUser:ClickButton2(Vector2.new())
        end)
        AntiAFKToggle.Text = "ON"
        AntiAFKToggle.BackgroundColor3 = Color3.fromRGB(40, 167, 69)
    else
        if Connections.AntiAFK then
            Connections.AntiAFK:Disconnect()
        end
        AntiAFKToggle.Text = "OFF"
        AntiAFKToggle.BackgroundColor3 = Color3.fromRGB(220, 53, 69)
    end
end

local function ToggleFOV()
    CheatStates.FOVChanger = not CheatStates.FOVChanger
    if CheatStates.FOVChanger then
        local fov = tonumber(FOVInput.Text) or 70
        Camera.FieldOfView = fov
        FOVToggle.Text = "ON"
        FOVToggle.BackgroundColor3 = Color3.fromRGB(40, 167, 69)
    else
        Camera.FieldOfView = OriginalFOV
        FOVToggle.Text = "OFF"
        FOVToggle.BackgroundColor3 = Color3.fromRGB(220, 53, 69)
    end
end

local function ToggleWalkOnWater()
    CheatStates.WalkOnWater = not CheatStates.WalkOnWater
    if CheatStates.WalkOnWater then
        Connections.WalkOnWater = RunService.Heartbeat:Connect(function()
            local character = LocalPlayer.Character
            if character and character:FindFirstChild("HumanoidRootPart") then
                for _, obj in pairs(Workspace:GetDescendants()) do
                    if obj.Name == "Water" or obj.Material == Enum.Material.Water then
                        obj.CanCollide = true
                    end
                end
            end
        end)
        WaterWalkToggle.Text = "ON"
        WaterWalkToggle.BackgroundColor3 = Color3.fromRGB(40, 167, 69)
    else
        if Connections.WalkOnWater then
            Connections.WalkOnWater:Disconnect()
        end
        for _, obj in pairs(Workspace:GetDescendants()) do
            if obj.Name == "Water" or obj.Material == Enum.Material.Water then
                obj.CanCollide = false
            end
        end
        WaterWalkToggle.Text = "OFF"
        WaterWalkToggle.BackgroundColor3 = Color3.fromRGB(220, 53, 69)
    end
end

local function ToggleInfiniteYield()
    CheatStates.InfiniteYield = not CheatStates.InfiniteYield
    if CheatStates.InfiniteYield then
        -- –ó–∞–≥—Ä—É–∑–∫–∞ Infinite Yield
        loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
        InfYieldToggle.Text = "ON"
        InfYieldToggle.BackgroundColor3 = Color3.fromRGB(40, 167, 69)
    else
        InfYieldToggle.Text = "OFF"
        InfYieldToggle.BackgroundColor3 = Color3.fromRGB(220, 53, 69)
    end
end

-- –ü–æ–¥–∫–ª—é—á–µ–Ω–∏–µ –≤—Å–µ—Ö –∫–Ω–æ–ø–æ–∫
SpeedToggle.MouseButton1Click:Connect(ToggleSpeed)
JumpToggle.MouseButton1Click:Connect(ToggleJumpPower)
FlyToggle.MouseButton1Click:Connect(ToggleFly)
NoclipToggle.MouseButton1Click:Connect(ToggleNoclip)
ESPToggle.MouseButton1Click:Connect(ToggleESP)
FullbrightToggle.MouseButton1Click:Connect(ToggleFullbright)
InfJumpToggle.MouseButton1Click:Connect(ToggleInfiniteJump)
ClickTPToggle.MouseButton1Click:Connect(ToggleClickTP)
SpinbotToggle.MouseButton1Click:Connect(ToggleSpinbot)
GodmodeToggle.MouseButton1Click:Connect(ToggleGodmode)
AutoRespawnToggle.MouseButton1Click:Connect(ToggleAutoRespawn)
AntiAFKToggle.MouseButton1Click:Connect(ToggleAntiAFK)
FOVToggle.MouseButton1Click:Connect(ToggleFOV)
WaterWalkToggle.MouseButton1Click:Connect(ToggleWalkOnWater)
InfYieldToggle.MouseButton1Click:Connect(ToggleInfiniteYield)

-- –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏
-- –ë—ã—Å—Ç—Ä—ã–µ –∫–æ–º–∞–Ω–¥—ã —á–µ—Ä–µ–∑ —á–∞—Ç
local function ProcessCommand(message)
    local args = string.split(message, " ")
    local cmd = args[1]:lower()
    
    if cmd == "/speed" and args[2] then
        local character = LocalPlayer.Character
        if character and character:FindFirstChild("Humanoid") then
            character.Humanoid.WalkSpeed = tonumber(args[2]) or 16
        end
    elseif cmd == "/jump" and args[2] then
        local character = LocalPlayer.Character
        if character and character:FindFirstChild("Humanoid") then
            character.Humanoid.JumpPower = tonumber(args[2]) or 50
        end
    elseif cmd == "/fov" and args[2] then
        Camera.FieldOfView = tonumber(args[2]) or 70
    elseif cmd == "/tp" and args[2] then
        local targetPlayer = Players:FindFirstChild(args[2])
        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local character = LocalPlayer.Character
            if character and character:FindFirstChild("HumanoidRootPart") then
                character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame
            end
        end
    elseif cmd == "/bring" and args[2] then
        local targetPlayer = Players:FindFirstChild(args[2])
        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local character = LocalPlayer.Character
            if character and character:FindFirstChild("HumanoidRootPart") then
                targetPlayer.Character.HumanoidRootPart.CFrame = character.HumanoidRootPart.CFrame
            end
        end
    elseif cmd == "/kill" and args[2] then
        local targetPlayer = Players:FindFirstChild(args[2])
        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Humanoid") then
            targetPlayer.Character.Humanoid.Health = 0
        end
    end
end

-- –ü–æ–¥–∫–ª—é—á–µ–Ω–∏–µ –∫–æ–º–∞–Ω–¥ —á–∞—Ç–∞
LocalPlayer.Chatted:Connect(ProcessCommand)

-- –ö–Ω–æ–ø–∫–∏ —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è –æ–∫–Ω–æ–º
CloseButton.MouseButton1Click:Connect(function()
    -- –û—Ç–∫–ª—é—á–∞–µ–º –≤—Å–µ —á–∏—Ç—ã –ø—Ä–∏ –∑–∞–∫—Ä—ã—Ç–∏–∏
    for name, state in pairs(CheatStates) do
        if state then
            CheatStates[name] = false
        end
    end
    for _, connection in pairs(Connections) do
        if connection then
            connection:Disconnect()
        end
    end
    ScreenGui:Destroy()
end)

local minimized = false
MinimizeButton.MouseButton1Click:Connect(function()
    minimized = not minimized
    if minimized then
        MainFrame:TweenSize(UDim2.new(0, 320, 0, 32), "Out", "Quad", 0.3, true)
        Shadow:TweenSize(UDim2.new(0, 320, 0, 32), "Out", "Quad", 0.3, true)
        ScrollFrame.Visible = false
    else
        MainFrame:TweenSize(UDim2.new(0, 320, 0, 420), "Out", "Quad", 0.3, true)
        Shadow:TweenSize(UDim2.new(0, 320, 0, 420), "Out", "Quad", 0.3, true)
        wait(0.3)
        ScrollFrame.Visible = true
    end
end)

-- –ì–æ—Ä—è—á–∏–µ –∫–ª–∞–≤–∏—à–∏
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    
    if input.KeyCode == Enum.KeyCode.Insert then
        ScreenGui.Enabled = not ScreenGui.Enabled
    elseif input.KeyCode == Enum.KeyCode.Delete then
        -- –ü–∞–Ω–∏–∫–∞! –û—Ç–∫–ª—é—á–∏—Ç—å –≤—Å–µ —á–∏—Ç—ã
        for name, state in pairs(CheatStates) do
            if state then
                CheatStates[name] = false
            end
        end
        for _, connection in pairs(Connections) do
            if connection then
                connection:Disconnect()
            end
        end
        -- –í–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã—Ö –∑–Ω–∞—á–µ–Ω–∏–π
        local character = LocalPlayer.Character
        if character then
            if character:FindFirstChild("Humanoid") then
                character.Humanoid.WalkSpeed = 16
                character.Humanoid.JumpPower = 50
                character.Humanoid.MaxHealth = 100
                character.Humanoid.Health = 100
            end
            if character:FindFirstChild("HumanoidRootPart") and character.HumanoidRootPart:FindFirstChild("BodyVelocity") then
                character.HumanoidRootPart.BodyVelocity:Destroy()
            end
        end
        Camera.FieldOfView = OriginalFOV
        -- –°–±—Ä–æ—Å –≤–∏–∑—É–∞–ª—å–Ω–æ–≥–æ —Å–æ—Å—Ç–æ—è–Ω–∏—è –∫–Ω–æ–ø–æ–∫
        for _, frame in pairs(ScrollFrame:GetChildren()) do
            if frame:FindFirstChild("ToggleButton") then
                frame.ToggleButton.Text = "OFF"
                frame.ToggleButton.BackgroundColor3 = Color3.fromRGB(220, 53, 69)
            end
        end
    elseif input.KeyCode == Enum.KeyCode.Home then
        -- –ë—ã—Å—Ç—Ä–æ–µ –≤–∫–ª—é—á–µ–Ω–∏–µ –æ—Å–Ω–æ–≤–Ω—ã—Ö —á–∏—Ç–æ–≤
        if not CheatStates.Speed then ToggleSpeed() end
        if not CheatStates.Fly then ToggleFly() end
        if not CheatStates.Noclip then ToggleNoclip() end
        if not CheatStates.InfiniteJump then ToggleInfiniteJump() end
    end
end)

-- –ê–≤—Ç–æ—Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –ø–æ–∑–∏—Ü–∏–π –∏–≥—Ä–æ–∫–æ–≤ –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
local PlayerPositions = {}
spawn(function()
    while wait(1) do
        for _, player in pairs(Players:GetPlayers()) do
            if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                PlayerPositions[player.Name] = player.Character.HumanoidRootPart.Position
            end
        end
    end
end)

-- –°–∏—Å—Ç–µ–º–∞ —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–π
local function CreateNotification(text, color, duration)
    local NotifGui = Instance.new("ScreenGui")
    local NotifFrame = Instance.new("Frame")
    local NotifLabel = Instance.new("TextLabel")
    
    NotifGui.Name = "Notification"
    NotifGui.Parent = LocalPlayer.PlayerGui
    
    NotifFrame.Parent = NotifGui
    NotifFrame.BackgroundColor3 = color or Color3.fromRGB(50, 50, 70)
    NotifFrame.BorderSizePixel = 0
    NotifFrame.Position = UDim2.new(1, 0, 0, 50)
    NotifFrame.Size = UDim2.new(0, 250, 0, 40)
    
    local NotifCorner = Instance.new("UICorner")
    NotifCorner.CornerRadius = UDim.new(0, 8)
    NotifCorner.Parent = NotifFrame
    
    NotifLabel.Parent = NotifFrame
    NotifLabel.BackgroundTransparency = 1
    NotifLabel.Size = UDim2.new(1, 0, 1, 0)
    NotifLabel.Font = Enum.Font.GothamSemibold
    NotifLabel.Text = text
    NotifLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    NotifLabel.TextSize = 12
    
    -- –ê–Ω–∏–º–∞—Ü–∏—è –ø–æ—è–≤–ª–µ–Ω–∏—è
    NotifFrame:TweenPosition(UDim2.new(1, -260, 0, 50), "Out", "Quad", 0.5, true)
    
    -- –ò—Å—á–µ–∑–Ω–æ–≤–µ–Ω–∏–µ
    wait(duration or 3)
    NotifFrame:TweenPosition(UDim2.new(1, 0, 0, 50), "Out", "Quad", 0.5, true)
    wait(0.5)
    NotifGui:Destroy()
end

-- –ü—Ä–∏–≤–µ—Ç—Å—Ç–≤–µ–Ω–Ω–æ–µ —Å–æ–æ–±—â–µ–Ω–∏–µ
spawn(function()
    wait(1)
    CreateNotification("üî• ROBLOX HACKS –∑–∞–≥—Ä—É–∂–µ–Ω–æ!", Color3.fromRGB(40, 167, 69), 4)
    wait(1)
    CreateNotification("INSERT - –º–µ–Ω—é | DELETE - –ø–∞–Ω–∏–∫–∞ | HOME - –±—ã—Å—Ç—Ä—ã–µ —á–∏—Ç—ã", Color3.fromRGB(255, 193, 7), 6)
    wait(1)
    CreateNotification("CTRL+Click - —Ç–µ–ª–µ–ø–æ—Ä—Ç | –ö–æ–º–∞–Ω–¥—ã —á–∞—Ç–∞ –¥–æ—Å—Ç—É–ø–Ω—ã", Color3.fromRGB(0, 123, 255), 5)
end)


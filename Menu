    ESP = false,
    Fullbright = false,
    InfiniteJump = false,
    ClickTP = false,
    Spinbot = false,
    AutoRespawn = false,
    AntiAFK = false,
    FOVChanger = false,
    WalkOnWater = false,
    Godmode = false,
    InfiniteYield = false
}

